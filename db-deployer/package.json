{
  "name": "pg-beershop-db-deployer",
  "engines": {
    "node": "12"
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "start:deploy": "npx cds-dbm deploy"
  },
  "dependencies": {
    "cds-dbm": "^0.0.10",
    "cds-pg": "^0.0.20",
    "express": "^4.17.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "cds": {
    "requires": {
      "db": {
        "kind": "PostgreSQL"
      },
      "PostgreSQL": {
        "impl": "cds-pg",
        "model": [
          "srv"
        ]
      },
      "uaa": {
        "kind": "xsuaa"
      }
    },
    "migrations": {
      "db": {
        "schema": {
          "default": "public",
          "clone": "_cdsdbm_clone",
          "reference": "_cdsdbm_ref"
        },
        "deploy": {
          "tmpFile": "tmp/_autodeploy.json",
          "undeployFile": "db/undeploy.json"
        }
      }
    }
  }
}
